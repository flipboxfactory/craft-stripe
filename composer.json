{
  "name": "flipboxfactory/craft-stripe",
  "description": "Stripe Integration for Craft CMS",
  "version": "1.1.0",
  "type": "craft-plugin",
  "license": "proprietary",
  "keywords": [
    "cms",
    "craftcms",
    "stripe",
    "force.com",
    "yii2"
  ],
  "require": {
    "craftcms/cms": "^3.2",
    "stripe/stripe-php": "^6.0",
    "flipboxfactory/craft-ember": "^2.1",
    "flipboxfactory/craft-integration": "^2.2.2",
    "flipboxfactory/craft-psr3": "^1.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4",
    "codeception/codeception": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\craft\\stripe\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\craft\\stripe\\tests\\": "tests/src/"
    }
  },
  "support": {
    "email": "support@flipboxfactory.com",
    "issues": "https://github.com/flipboxfactory/craft-stripe/issues?state=open",
    "source": "https://github.com/flipboxfactory/craft-stripe",
    "docs": "https://github.com/flipboxfactory/craft-stripe",
    "rss": "https://github.com/flipboxfactory/craft-stripe/commits/master.atom"
  },
  "extra": {
    "name": "Stripe",
    "handle": "stripe",
    "class": "flipbox\\craft\\stripe\\Stripe",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/craft-stripe/master/CHANGELOG.md",
    "documentationUrl": "https://stripe.flipboxfactory.com",
    "hasCpSection": true,
    "hasCpSettings": true,
    "schemaVersion": "1.2.0",
    "branch-alias": {
      "dev-master": "1.0.x-dev",
      "dev-develop": "1.0.x-dev"
    }
  }
}